<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="EngagePerformance" parallel="tests" thread-count="2">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<test name="Platform">
		<groups>
			<run>
				<include name="Platform" />
			</run>
		</groups>
		<classes>
			<class name="com.engage.performance.EngagePerformance_TC" />
		</classes>
	</test>
	
</suite>
